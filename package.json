{
  "name": "travel-ui",
  "version": "1.0.0",
  "description": "My wonderful travel project",
  "author": "mehmetkocerr",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
