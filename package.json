    "element-ui": "^2.4.11",
    "express": "^4.17.3",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "node-sass": "^7.0.0",
    "nuxt": "^2.0.0",
    "sass-loader": "^8.0.0",
    "tailwindcss": "^1.1.3",
    "vue-moment": "^4.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
